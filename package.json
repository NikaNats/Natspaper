{
  "name": "natspaper",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=development astro dev",
    "build": "cross-env NODE_ENV=production astro check && astro build && npm run copy-katex && pagefind --site dist && npm run copy-pagefind && npm run verify-build",
    "build:dev": "cross-env NODE_ENV=development astro check && astro build",
    "build:prod": "cross-env NODE_ENV=production astro check && astro build && npm run copy-katex && pagefind --site dist && npm run copy-pagefind && npm run verify-build",
    "preview": "cross-env NODE_ENV=production astro preview",
    "verify-build": "node scripts/verify-build.js",
    "copy-pagefind": "node scripts/copy-pagefind.js",
    "copy-katex": "node scripts/copy-katex.js",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:workflows": "vitest run tests/workflows",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:all": "pnpm test:run && pnpm test:e2e"
  },
  "dependencies": {
    "@astrojs/rss": "^4.0.13",
    "@astrojs/sitemap": "^3.6.0",
    "@resvg/resvg-js": "^2.6.2",
    "@sentry/astro": "^10.22.0",
    "@sentry/tracing": "^7.120.4",
    "@tailwindcss/vite": "^4.1.16",
    "astro": "^5.15.1",
    "dayjs": "^1.11.18",
    "katex": "^0.16.25",
    "lodash.kebabcase": "^4.1.1",
    "rehype-katex": "^7.0.1",
    "remark-collapse": "^0.1.2",
    "remark-math": "^6.0.0",
    "remark-toc": "^9.0.0",
    "satori": "^0.18.3",
    "satori-html": "^0.3.2",
    "sharp": "^0.34.4",
    "tailwindcss": "^4.1.16"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.5",
    "@pagefind/default-ui": "^1.4.0",
    "@playwright/test": "^1.56.1",
    "@shikijs/transformers": "^3.14.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/lodash.kebabcase": "^4.1.9",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/ui": "^4.0.3",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "eslint-plugin-astro": "^1.3.1",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.8",
    "pagefind": "^1.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.3"
  }
}

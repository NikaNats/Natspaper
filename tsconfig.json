{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "pagefind.d.ts", "**/*"],
  "exclude": ["dist", "public/pagefind", "coverage"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@tests/*": ["./tests/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true
  }
}
